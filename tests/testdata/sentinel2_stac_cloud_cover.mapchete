process: read_xarray.py
input:
  inp:
    format: Sentinel-2
    level: L2A
    time:
      start: 2023-08-10
      end: 2023-08-13
    max_cloud_cover: 20      
    cat_baseurl: sentinel2/full_products/catalog.json
output:
  format: GTiff
  bands: 3
  path: stac
  dtype: uint16
pyramid:
  grid: geodetic
zoom_levels: 13
bounds: [16, 46, 16.1, 46.1]