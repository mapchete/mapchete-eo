process: read_xarray.py
input:
  inp:
    format: Sentinel-2
    level: L2A
    archive: S2AWS_JP2
    search_index: s2-jp2-fgb-catalog/index.fgb
    max_cloud_percent: 90
    time:
      start: 2023-06-06
      end: 2023-06-06
output:
  format: GTiff
  bands: 3
  path: sentinel2_static_catalog
  dtype: uint16
pyramid:
  grid: geodetic
zoom_levels: 13
bounds: [16, 46, 17, 47]