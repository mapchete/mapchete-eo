process: read_xarray.py
input:
  sentinel2:
    format: Sentinel-2
    cat_baseurl: sentinel2/full_products_antimeridian/catalog.json
    level: L2A
    time:    
        start: 2023-06-01
        end: 2023-06-10
output:
  format: GTiff
  bands: 3
  path: sentinel2_antimeridian_east
  dtype: uint16
pyramid:
  grid: geodetic
zoom_levels: 10
bounds: [179.5, 67, 180, 67.3]
