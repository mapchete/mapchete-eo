process: mapchete_eo.processes.eoxcloudless_mosaic
input:
  sentinel2:
    format: Sentinel-2
    level: L2A
    time:
      start: 2023-07-01
      end: 2023-07-06
    with_cloudmasks: true
output:
  format: GTiff
  bands: 4
  path: eoxcloudless_tmp
  dtype: uint16
pyramid:
  grid: geodetic
zoom_levels: 13
# tmx bounds 13 1879 8938
bounds: [16.3916015625, 48.69140625, 16.41357421875, 48.71337890625]

process_parameters:
  target_height: 15
  assets: ["red", "green", "blue", "nir"]
  method: brightness
  from_brightness_extract_method: third_quartile